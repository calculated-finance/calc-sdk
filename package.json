{
  "name": "@calc_fi/strategy-builder",
  "version": "0.1.8",
  "description": "Type-safe strategy builder for CALC",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./calc": "./dist/calc.d.ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "bun test",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "npm pkg fix && bun run typecheck && bun test && bun run build"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=18",
    "bun": ">=1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calculated-finance/calc-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/calculated-finance/calc-sdk/issues"
  },
  "homepage": "https://github.com/calculated-finance/calc-sdk#readme",
  "keywords": [
    "calc",
    "sdk",
    "strategy",
    "builder",
    "cosmwasm",
    "rujira",
    "thorchain"
  ],
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.5.0"
  }
}